.ranking-area
  width 100%
  height 100%
  overflow-y auto
  padding-top 10px
  display flex
  flex-direction column
  justify-content center
  > div
      display flex
      flex-direction row
      padding 2px 0
      img
        flex 5
        width 0
        height 130px
      ul
        flex 8
        display flex
        flex-direction column
        justify-content space-around
        list-style none
        color #616765
        padding-left 10px
        border-bottom 1px solid #d6dad9








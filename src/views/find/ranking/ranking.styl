.ranking-area
  background-color #fff
  width 100%
  height 100%
  overflow-y auto
  padding-top 10px
  display flex
  flex-direction column
  justify-content center
  > div
      display flex
      flex-direction row
      padding 5px 0
      img
        flex 5
      ul
        flex 8
        display flex
        flex-direction column
        justify-content space-around
        list-style none







